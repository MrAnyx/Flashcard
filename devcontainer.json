{
    "name": "Flashcard API",
    "forwardPorts": [
        8080,
        8081,
        8082
    ],
    "portsAttributes": {
        "8080": {
            "label": "Api"
        },
        "8081": {
            "label": "PhpMyAdmin"
        },
        "8082": {
            "label": "Mailer"
        }
    },
    "dockerComposeFile": "docker-compose.yaml",
    "service": "api",
    "workspaceFolder": "/var/www",
    "postCreateCommand": "task app:default:config",
    "features": {
        "ghcr.io/devcontainers/features/github-cli:1": {
            "version": "latest"
        }
    }
}
